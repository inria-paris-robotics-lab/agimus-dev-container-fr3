{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/**",
                "/opt/ros/${env:ROS_DISTRO}/include/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/usr/include",
                    "/opt/ros/${env:ROS_DISTRO}/include"
